#include <iostream>

using namespace std;

int main() {
    int num1, num2;
    int *ptr1, *ptr2;

    cout << "Enter two numbers: ";
    cin >> num1 >> num2;

    ptr1 = &num1;
    ptr2 = &num2;

    if (*ptr1 > *ptr2) {
        cout << "Maximum number is " << *ptr1 << endl;
    } else {
        cout << "Maximum number is " << *ptr2 << endl;
    }

    return 0;
}
